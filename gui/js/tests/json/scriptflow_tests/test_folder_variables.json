{
  "ff_scale": {
    "description": "Scale video with folder name",
    "input": "../../../tests/samples/sample_video.mp4",
    "output": "scaled_<FOLDER_NAME>.mp4",
    "width": 640,
    "height": 480
  },
  "ff_text": {
    "description": "Add text with folder title",
    "input": "scaled_<FOLDER_NAME>.mp4",
    "output": "text_<FOLDER_TITLE>.mp4",
    "text": "Welcome to <FOLDER_TITLE>",
    "x": "10",
    "y": "10",
    "fontsize": "24",
    "color": "<CONSTANT_RANDOM_COLOUR>"
  },
  "ff_watermark": {
    "description": "Add watermark with folder info",
    "input": "text_<FOLDER_TITLE>.mp4",
    "watermark": "../../../tests/samples/sample_image.png",
    "output": "final_<FOLDER_NAME>.mp4",
    "xpixels": "10",
    "ypixels": "10",
    "scale": "0.2",
    "alpha": "0.7"
  }
}
