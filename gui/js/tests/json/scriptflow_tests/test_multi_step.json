{
  "ff_scale": {
    "description": "Scale video",
    "input": "../../../tests/samples/sample_video.mp4",
    "output": "scaled.mp4",
    "width": 640,
    "height": 480
  },
  "ff_text": {
    "description": "Add text",
    "input": "scaled.mp4",
    "output": "text.mp4",
    "text": "Multi-step test",
    "x": "10",
    "y": "10",
    "fontsize": "24",
    "color": "<CONSTANT_RANDOM_COLOUR>"
  },
  "ff_watermark": {
    "description": "Add watermark",
    "input": "text.mp4",
    "watermark": "../../../tests/samples/sample_image.png",
    "output": "final.mp4",
    "xpixels": "10",
    "ypixels": "10",
    "scale": "0.2",
    "alpha": "0.7"
  },
  "ff_unsharp": {
    "description": "Apply unsharp mask",
    "input": "final.mp4",
    "output": "unsharped.mp4",
    "la": "1.5"
  }
}
