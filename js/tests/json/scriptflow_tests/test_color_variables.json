{
  "ff_scale": {
    "description": "Scale video with random colors",
    "input": "../../../tests/samples/sample_video.mp4",
    "output": "scaled_random.mp4",
    "width": 640,
    "height": 480
  },
  "ff_text": {
    "description": "Add text with random contrast color",
    "input": "scaled_random.mp4",
    "output": "text_contrast.mp4",
    "text": "Random Colors Test",
    "x": "10",
    "y": "10",
    "fontsize": "24",
    "color": "<RANDOM_COLOUR>"
  },
  "ff_watermark": {
    "description": "Add watermark with constant colors",
    "input": "text_contrast.mp4",
    "watermark": "../../../tests/samples/sample_image.png",
    "output": "final_constant.mp4",
    "xpixels": "10",
    "ypixels": "10",
    "scale": "0.2",
    "alpha": "0.7"
  }
}
